{
  "name": "copcop",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "yarn run typeorm -- migration:create",
    "migration:revert": "yarn run typeorm_local -- migration:revert",
    "migration:run": "yarn run typeorm_local -- migration:run",
    "migration:show": "yarn run typeorm_local -- migration:show",
    "migration:drop": "yarn run typeorm_local -- migration:drop",
    "typeorm_local": "yarn run build && typeorm -d src/database/data-source.js",
    "migration:generate": "npx tsc && npm run typeorm migration:generate userTable -- -d ./src/database/data-source.ts",
    "db-migrate": "npx tsc && npm run typeorm migration:run -- -d ./src/database/data-source.ts"
  },
  "dependencies": {
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.1",
    "@types/multer": "^1.4.11",
    "@types/pug": "^2.0.10",
    "@types/sinon": "^17.0.3",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.3",
    "glob": "^11.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.0.0",
    "nest-access-control": "^3.1.0",
    "nest-csv-parser": "^2.0.4",
    "nestjs-typeorm-paginate": "^4.0.4",
    "node-mailjet": "^6.0.5",
    "node-mocks-http": "^1.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-magic-login": "^1.2.2",
    "pg": "^8.11.3",
    "pug": "^3.0.2",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "superagent": "^9.0.1",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.3.1",
    "@types/node-mailjet": "^3.3.12",
    "@types/randomstring": "^1.1.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
